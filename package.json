{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate with custom webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --hot --progress --inline --config build/webpack.dev.js",
    "build:dev": "webpack --config build/webpack.prod.js --mode development",
    "build:prod": "webpack --config build/webpack.prod.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsit18/react-boilerplate.git"
  },
  "keywords": [
    "reactjs",
    "react-boilerplate"
  ],
  "author": "Harpreet Singh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hsit18/react-boilerplate/issues"
  },
  "homepage": "https://github.com/hsit18/react-boilerplate#readme",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^8.1.1",
    "dotenv": "^8.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-webpack-plugin": "^5.3.1",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "workbox-webpack-plugin": "^6.1.2"
  }
}
